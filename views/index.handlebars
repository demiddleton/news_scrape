<div class="row">
    <div class="col s2"></div>
    <div class="col s8 left-align">
        {{#each article}}
        <div class="card-panel">
            <h4>{{this.title}}</h4>
            <a href="/readArticle/{{this._id}}" class="btn btn-primary" id="btnSignUp">Read Article</a>

        </div>

        {{/each}}
    </div>
</div>